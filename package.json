{
	"name": "exp-decoder",
	"displayName": "EXP decoder",
	"description": "Decode SonicWall Firewall backup config ( EXP ) files",
	"version": "0.0.3",
	"publisher": "varunsudarshan",
	"repository": {
		"type": "git",
		"url": "https://github.com/varunsudarshan/exp-decoder.git"
		},
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.decodeEXP"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "extension.decodeEXP",
			"title": "Decode EXP"
		}],
		"keybindings": [
			{
				"command": "extension.decodeEXP",
				"key": "ctrl+r ctrl+d",
				"when": "editorTextFocus"
			}
		],
		"menus": {
			"editor/context": [
				{
				  "command": "extension.decodeEXP"
				}
			  ]
		  }
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.0.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
